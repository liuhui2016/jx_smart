package com.game.smvc.entity;

import java.util.Date;

import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;
import javax.persistence.Table;

@Entity
@Table(name = "jx_community_sale")
public class JxCommunitySale {

	private int id;
	private int u_id;//举报人id
	private int pub_id;//被举报人id
	private String rpt_name;//举报人昵称
	private String rpt_content;//举报内容
	private String rpt_phone;//举报人电话
	private String rpt_cause;//举报原因
	private int rpt_sex;//举报人性别
	private String rpd_name;//被举报商家名称
	private String rpd_phone;//被举报商家电话
	private String rpd_company;//被举报内容
	private int rpt_state;//举报状态
	private int rpt_number;//举报次数
	private Date rpt_addtime;
	private Date rpt_modtime;
	
	@Id
	@GeneratedValue(strategy = GenerationType.IDENTITY)
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public int getU_id() {
		return u_id;
	}
	public void setU_id(int u_id) {
		this.u_id = u_id;
	}
	public int getPub_id() {
		return pub_id;
	}
	public void setPub_id(int pub_id) {
		this.pub_id = pub_id;
	}
	public String getRpt_name() {
		return rpt_name;
	}
	public void setRpt_name(String rpt_name) {
		this.rpt_name = rpt_name;
	}
	public String getRpt_content() {
		return rpt_content;
	}
	public void setRpt_content(String rpt_content) {
		this.rpt_content = rpt_content;
	}
	public String getRpt_phone() {
		return rpt_phone;
	}
	public void setRpt_phone(String rpt_phone) {
		this.rpt_phone = rpt_phone;
	}
	public String getRpt_cause() {
		return rpt_cause;
	}
	public void setRpt_cause(String rpt_cause) {
		this.rpt_cause = rpt_cause;
	}
	public int getRpt_sex() {
		return rpt_sex;
	}
	public void setRpt_sex(int rpt_sex) {
		this.rpt_sex = rpt_sex;
	}
	public String getRpd_name() {
		return rpd_name;
	}
	public void setRpd_name(String rpd_name) {
		this.rpd_name = rpd_name;
	}
	public String getRpd_phone() {
		return rpd_phone;
	}
	public void setRpd_phone(String rpd_phone) {
		this.rpd_phone = rpd_phone;
	}
	public String getRpd_company() {
		return rpd_company;
	}
	public void setRpd_company(String rpd_company) {
		this.rpd_company = rpd_company;
	}
	public int getRpt_state() {
		return rpt_state;
	}
	public void setRpt_state(int rpt_state) {
		this.rpt_state = rpt_state;
	}
	public int getRpt_number() {
		return rpt_number;
	}
	public void setRpt_number(int rpt_number) {
		this.rpt_number = rpt_number;
	}
	public Date getRpt_addtime() {
		return rpt_addtime;
	}
	public void setRpt_addtime(Date rpt_addtime) {
		this.rpt_addtime = rpt_addtime;
	}
	public Date getRpt_modtime() {
		return rpt_modtime;
	}
	public void setRpt_modtime(Date rpt_modtime) {
		this.rpt_modtime = rpt_modtime;
	}
	
	
	
}
