package com.game.smvc.entity;
/**
 * 售后信息
 */
import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;
import javax.persistence.Table;

@Entity
@Table(name = "jx_after_sales")
public class JxAfterSales {

	private int as_id;//售后id
	private String ord_no;//售后订单号
	private String pro_no;//售后机器码
	private int u_id;//售后人id
	private String adr_id;//售后人地址
	private int pro_id;//机器类型
	private float ord_price;//价格
	private String as_managerno;//产品经理编号
	private String as_phone;//售后人电话
	private String ord_receivename;//售后人姓名
	private int as_status;//状态
	private String as_proname;//售后的机器名称
	private int as_visit;//售后回访状态：0未回访；1已回访
	private String as_color;//售后机器颜色
	private String pro_restflow;//剩余的流量
	private int pro_day;//剩余的天数
	private java.util.Date as_addtime;//添加时间
	private java.util.Date as_modtime;//更新时间
	
	@Id
	@GeneratedValue(strategy = GenerationType.IDENTITY)
	public int getAs_id() {
		return as_id;
	}
	public void setAs_id(int as_id) {
		this.as_id = as_id;
	}
	public String getOrd_no() {
		return ord_no;
	}
	public void setOrd_no(String ord_no) {
		this.ord_no = ord_no;
	}
	public String getPro_no() {
		return pro_no;
	}
	public void setPro_no(String pro_no) {
		this.pro_no = pro_no;
	}
	public int getU_id() {
		return u_id;
	}
	public void setU_id(int u_id) {
		this.u_id = u_id;
	}
	public String getAdr_id() {
		return adr_id;
	}
	public void setAdr_id(String adr_id) {
		this.adr_id = adr_id;
	}
	public int getPro_id() {
		return pro_id;
	}
	public void setPro_id(int pro_id) {
		this.pro_id = pro_id;
	}
	public float getOrd_price() {
		return ord_price;
	}
	public void setOrd_price(float ord_price) {
		this.ord_price = ord_price;
	}
	public String getAs_managerno() {
		return as_managerno;
	}
	public void setAs_managerno(String as_managerno) {
		this.as_managerno = as_managerno;
	}
	public String getAs_phone() {
		return as_phone;
	}
	public void setAs_phone(String as_phone) {
		this.as_phone = as_phone;
	}
	public String getOrd_receivename() {
		return ord_receivename;
	}
	public void setOrd_receivename(String ord_receivename) {
		this.ord_receivename = ord_receivename;
	}
	public int getAs_status() {
		return as_status;
	}
	public void setAs_status(int as_status) {
		this.as_status = as_status;
	}
	public String getAs_proname() {
		return as_proname;
	}
	public void setAs_proname(String as_proname) {
		this.as_proname = as_proname;
	}
	public int getAs_visit() {
		return as_visit;
	}
	public void setAs_visit(int as_visit) {
		this.as_visit = as_visit;
	}
	public String getAs_color() {
		return as_color;
	}
	public void setAs_color(String as_color) {
		this.as_color = as_color;
	}
	public String getPro_restflow() {
		return pro_restflow;
	}
	public void setPro_restflow(String pro_restflow) {
		this.pro_restflow = pro_restflow;
	}
	public int getPro_day() {
		return pro_day;
	}
	public void setPro_day(int pro_day) {
		this.pro_day = pro_day;
	}
	public java.util.Date getAs_addtime() {
		return as_addtime;
	}
	public void setAs_addtime(java.util.Date as_addtime) {
		this.as_addtime = as_addtime;
	}
	public java.util.Date getAs_modtime() {
		return as_modtime;
	}
	public void setAs_modtime(java.util.Date as_modtime) {
		this.as_modtime = as_modtime;
	}
	
	
}
